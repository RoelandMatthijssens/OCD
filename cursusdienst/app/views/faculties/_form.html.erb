<%= form_for(@faculty) do |f| %>

  <div class='field'>
    <%= f.label :name %> <br>
    <%= f.text_field :name %> <br>
  </div>
  <div class='field'>
    <%= f.label :initials %> <br>
    <%= f.text_field :initials %> <br>
  </div>
  <div class="actions">
    <%= f.submit @submit %>
  </div>
<% end %>