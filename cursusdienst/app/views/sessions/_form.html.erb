<%= form_for(:session, :url => sessions_path) do |f| %><br/>
  <%= render :partial => 'error_messages', :locals => {:errors => @errors} %>
  <div class='field'>
    <%= f.label :user_name %> <br>
    <%= f.text_field :user_name %> <br>
  </div>
  <div class='field'>
    <%= f.label :password %> <br>
    <%= f.password_field :password %> <br>
  </div>
  <div class="actions">
    <%= f.submit 'Sign in' %>
  </div>
<% end %>
