<h1><%= @title %></h1>
<h2>Label: <%= @label %> </h2>
<table>
  <thead>
    <tr>
      <th><%= t(:status, :scope => "globals" ) %></th>
      <th><%= t(:order_content , :scope => "globals" ) %></th>
      <th><%= t(:actions , :scope => "globals" ) %></th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order|%>
      <%= render order %>
    <% end %>
  </tbody>
</table>

<%= render :partial => "shared/side_order_processing" %>
